<h1>User: <%= @user.email %></h1>
<h2>Events Created by <%= @user.email %>:</h2>
<ul>
  <% @created_events.each do |event| %>
    <li>
      <h3><%= event.title %></h3>
      <p><%= event.description %></p>
      <p>Date: <%= event.date %></p>
      <p>Location: <%= event.location %></p>
    </li>
  <% end %>
</ul>

